{
  "name": "cylindergrid",
  "version": "0.1.0",
  "description": "Ashfall Lab 3D Carousel - A performant, accessible cylindrical carousel component",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "analyze": "vite build --mode analyze"
